{
  "$schema": "https://design-tokens.github.io/format/tokens.schema.json",
  "$description": "Structure semantic API tokens - context-aware tokens for layout, spacing, and borders",
  "structure": {
    "$description": "Structure tokens define spatial relationships and visual boundaries",
    "spacing": {
      "$description": "Spacing tokens for consistent layout rhythm",
      "inline": {
        "xxs": {
          "$value": "@structure.spacing.inline.xxs",
          "$type": "dimension",
          "$description": "Extra extra small inline spacing"
        },
        "xs": {
          "$value": "@structure.spacing.inline.xs",
          "$type": "dimension",
          "$description": "Extra small inline spacing"
        },
        "sm": {
          "$value": "@structure.spacing.inline.sm",
          "$type": "dimension",
          "$description": "Small inline spacing"
        },
        "md": {
          "$value": "@structure.spacing.inline.md",
          "$type": "dimension",
          "$description": "Medium inline spacing"
        },
        "lg": {
          "$value": "@structure.spacing.inline.lg",
          "$type": "dimension",
          "$description": "Large inline spacing"
        },
        "xl": {
          "$value": "@structure.spacing.inline.xl",
          "$type": "dimension",
          "$description": "Extra large inline spacing"
        }
