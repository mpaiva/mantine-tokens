          "$value": "@structure.border.radius.xl",
          "$type": "dimension",
          "$description": "Extra large border radius"
        },
        "full": {
          "$value": "@structure.border.radius.full",
          "$type": "dimension",
          "$description": "Full border radius (pill)"
        }
      }
