        },
        "md": {
          "$value": "@structure.spacing.stack.md",
          "$type": "dimension",
          "$description": "Medium stack spacing"
        },
        "lg": {
          "$value": "@structure.spacing.stack.lg",
          "$type": "dimension",
          "$description": "Large stack spacing"
        },
        "xl": {
          "$value": "@structure.spacing.stack.xl",
          "$type": "dimension",
          "$description": "Extra large stack spacing"
        }
      },
      "inset": {
        "xs": {
          "$value": "@structure.spacing.inset.xs",
          "$type": "dimension",
          "$description": "Extra small inset spacing"
        },
        "sm": {
          "$value": "@structure.spacing.inset.sm",
          "$type": "dimension",
          "$description": "Small inset spacing"
